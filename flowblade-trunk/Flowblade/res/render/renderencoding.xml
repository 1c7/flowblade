<renderencoding version="0.1">

    <!-- quality qroups are groups of quality options presented with an encoding option -->
    <!-- addargs="arg1=val1 arg2=va2"(space separated list) -->
    <!-- replvalues="replexp1 replval1;replexp2 replval2" (; separated list) -->
    <qualityqroup id="qgroup1" defaultindex="6">
        <quality name="200 kb/s" replvalues="%BITRATE% 200k" />
        <quality name="400 kb/s" replvalues="%BITRATE% 400k" />
        <quality name="600 kb/s" replvalues="%BITRATE% 600k" />
        <quality name="800 kb/s" replvalues="%BITRATE% 800k" />
        <quality name="1000 kb/s" replvalues="%BITRATE% 1000k" />
        <quality name="2000 kb/s" replvalues="%BITRATE% 2000k" />
        <quality name="3000 kb/s" replvalues="%BITRATE% 3000k" />
        <quality name="4000 kb/s" replvalues="%BITRATE% 4000k" />
        <quality name="5000 kb/s" replvalues="%BITRATE% 5000k" />
        <quality name="6000 kb/s" replvalues="%BITRATE% 6000k" />
        <quality name="8000 kb/s" replvalues="%BITRATE% 8000k" />
        <quality name="10000 kb/s" replvalues="%BITRATE% 10000k" />
        <quality name="12000 kb/s" replvalues="%BITRATE% 12000k" />
        <quality name="18000 kb/s" replvalues="%BITRATE% 18000k" />
        <quality name="20000 kb/s" replvalues="%BITRATE% 20000k" />
        <quality name="25000 kb/s" replvalues="%BITRATE% 25000k" />
    </qualityqroup>    
    <qualityqroup id="lossless">
        <quality name="lossless"/>
    </qualityqroup>
    <qualityqroup id="variable">
        <quality name="variable 1 pass"/>
        <quality name="variable 2 pass" addargs="pass=2"/>
    </qualityqroup>
    <qualityqroup id="variablenooption">
        <quality name="variable 1 pass"/>
    </qualityqroup>
    <qualityqroup id="not_settable">
        <quality name="not settable"/>
    </qualityqroup>
    
    <!-- encoding options group together codecs, file formats and quality option groups -->
    <!-- Actual encoding incantations lifted from Kdenlive -->
    <encodingoption name="MPEG-2 / .mpg" extension="mpg" audiodesc="mp2, 348kb/s, 48000Hz" type="av" resize="True" qgroup="qgroup1">
        <profile args="f=mpeg acodec=mp2 ab=384k ar=48000 vcodec=mpeg2video minrate=0 b=%BITRATE% bf=2 b_strategy=1 trellis=1" />
    </encodingoption>
    <encodingoption name=" pimppa/ .mpg" extension="mpg" audiodesc="mp2, 348kb/s, 48000Hz" type="av" resize="True" qgroup="qgroup1">
        <profile args="f=mpeg acodec=mp2 ab=384k ar=48000 vcodec=pimppa minrate=0 b=%BITRATE% bf=2 b_strategy=1 trellis=1" />
    </encodingoption>
    <!--- 
    <encodingoption name="H.264 / .mp4" extension="mp4" audiodesc="mp2, 348kb/s, 48000Hz" type="av" resize="True" qgroup="qgroup1">
        <profile args="f=mp4 s=%SCREENSIZE% hq=1 acodec=mp2 ab=384k ar=48000 pix_fmt=yuv420p vcodec=libx264 minrate=0 b=%BITRATE% b_strategy=1 subcmp=2 cmp=2 coder=1 flags=+loop flags2=dct8x8 qmax=51 subq=7 qmin=10 qcomp=0.6 qdiff=4 trellis=1 aspect=%ASPECT%"/>
    </encodingoption>
    <encodingoption name="H.264 / .avi" extension="avi" audiodesc="mp2, 348kb/s, 48000Hz" type="av" resize="True" qgroup="qgroup1">
        <profile args="f=mp4 s=%SCREENSIZE% hq=1 acodec=mp2 ab=384k ar=48000 pix_fmt=yuv420p vcodec=libx264 minrate=0 b=%BITRATE% b_strategy=1 subcmp=2 cmp=2 coder=1 flags=+loop flags2=dct8x8 qmax=51 subq=7 qmin=10 qcomp=0.6 qdiff=4 trellis=1 aspect=%ASPECT%"/>
    </encodingoption>
    -->
    <encodingoption name="MPEG-4 / .mov" extension="mov" type="av" audiodesc="mp2, 192kb/s, 48000Hz" resize="True" qgroup="qgroup1">
        <profile args="f=mp4 s=%SCREENSIZE% hq=1 acodec=mp2 ab=192k ar=48000 vcodec=mpeg4 minrate=0 b=%BITRATE%"/>
    </encodingoption>
    <encodingoption name="MPEG-4 / .avi" extension="avi" audiodesc="mp2, 192kb/s, 48000Hz" type="av" resize="True" qgroup="qgroup1">
        <profile args="f=mp4 s=%SCREENSIZE% hq=1 acodec=mp2 ab=192k ar=48000 vcodec=mpeg4 minrate=0 b=%BITRATE%"/>
    </encodingoption>
    <encodingoption name="Theora / .ogg" extension="ogg" audiodesc="flac lossless" type="av" resize="True" qgroup="qgroup1">
        <profile args="f=ogg acodec=flac ac=2 vcodec=libtheora minrate=0 b=%BITRATE%" />
    </encodingoption>
    <encodingoption name="DVD PAL 4:3 / .vob" audiodesc="ac3, 192kb/s, 48000Hz" extension="vob" type="av" resize="False" qgroup="variablenooption">
        <profile  args="f=dvd vcodec=mpeg2video acodec=ac3 b=5000k maxrate=8000k minrate=0 bufsize=1835008 mux_packet_s=2048 mux_rate=10080000 ab=192k ar=48000 s=720x576 g=15 me_range=63 trellis=1 profile=dv_pal" />
    </encodingoption>
    <encodingoption name="DVD PAL 16:9 / .vob" extension="vob" audiodesc="ac3, 192kb/s, 48000Hz" type="av" resize="False" qgroup="variablenooption">
        <profile args="f=dvd vcodec=mpeg2video acodec=ac3 b=5000k maxrate=8000k minrate=0 bufsize=1835008 mux_packet_s=2048 mux_rate=10080000 ab=192k ar=48000 s=720x576 g=15 me_range=63 trellis=1 profile=dv_pal_wide" />
    </encodingoption>
    <encodingoption name="DVD NTSC 4:3 / .vob" extension="vob" audiodesc="ac3, 192kb/s, 48000Hz" type="av" resize="False" qgroup="variablenooption">
        <profile args="f=dvd vcodec=mpeg2video acodec=ac3 s=720x480 b=6000k maxrate=9000k minrate=0 bufsize=1835008 mux_packet_s=2048 mux_rate=10080000 ab=192k ar=48000 g=18 me_range=63 trellis=1 profile=dv_ntsc" />
    </encodingoption>
    <encodingoption name="DVD NTSC 16:9 / .vob" extension="vob" audiodesc="ac3, 192kb/s, 48000Hz" type="av" resize="False" qgroup="variablenooption">
        <profile args="f=dvd vcodec=mpeg2video acodec=ac3 s=720x480 b=6000k maxrate=9000k minrate=0 bufsize=1835008 mux_packet_s=2048 mux_rate=10080000 ab=192k ar=48000 g=18 me_range=63 trellis=1 profile=dv_ntsc_wide" />
    </encodingoption>
    <!--- 
    <encodingoption name="DV PAL 4:3 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv420p acodec=pcm_s16le s=720x576 profile=dv_pal" />
    </encodingoption>
    <encodingoption name="DV PAL 16:9 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv420p acodec=pcm_s16le s=720x576 profile=dv_pal_wide" />
    </encodingoption>
    <encodingoption name="DV NTSC 4:3 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv411p acodec=pcm_s16le s=720x480 profile=dv_ntsc" />
    </encodingoption>
    <encodingoption name="DV NTSC 16:9 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv411p acodec=pcm_s16le s=720x480 profile=dv_ntsc_wide" />
    </encodingoption>
    <encodingoption name="DVCPRO50 PAL 4:3 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv422p acodec=pcm_s16le s=720x576 profile=dv_pal" />
    </encodingoption>
    <encodingoption name="DVCPRO50 PAL 16:9 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv422p acodec=pcm_s16le s=720x576 profile=dv_pal_wide" />
    </encodingoption>
    <encodingoption name="DVCPRO50 NTSC 4:3 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv422p acodec=pcm_s16le s=720x480 profile=dv_ntsc" />
    </encodingoption>
    <encodingoption name="DVCPRO50 NTSC 16:9 / .avi" extension="avi" audiodesc="pcm_s16le" type="av" resize="False" qgroup="not_settable">
        <profile args="f=avi vcodec=dvvideo pix_fmt=yuv422p acodec=pcm_s16le s=720x480 profile=dv_ntsc_wide" />
    </encodingoption>
    -->
    <!--- 
    <encodingoption name="Lossless H.264 / .mp4" extension="mp4" audiodesc="mp2, 384kb/s, 48000Hz"  type="av" resize="True" qgroup="lossless">
        <profile args="f=mp4 acodec=mp2 ab=384k ar=48000 ac=2 pix_fmt=yuv420p vcodec=libx264 cqp=0 me_method=esa subq=8 qmin=10 qcomp=0.6 qdiff=4 qmax=51 coder=ac partitions=+parti4x4+parti8x8+partp4x4+partp8x8+partb8x8 refs=16 flags2=+dct8x8+mixed_refs trellis=1" />
    </encodingoption>
    -->
    <encodingoption name="Lossless MPEG-4 / .mp4" extension="mp4" audiodesc="mp2, 384kb/s, 48000Hz" type="av" resize="True" qgroup="lossless">
        <profile args="f=mp4 acodec=mp2 ab=384k ar=48000 ac=2 vcodec=mpeg4 qscale=1 qmin=1" />
    </encodingoption>
    <encodingoption name="Lossless MPEG-2 / .mpg" audiodesc="mp2, 384kb/s, 48000Hz"  extension="mpg" type="av" resize="True" qgroup="lossless">
       <profile args="f=mpeg acodec=mp2 ab=384k ar=48000 ac=2 vcodec=mpeg2video qscale=1 qmin=1 aspect=%ASPECT% intra=1" />
   </encodingoption>

</renderencoding>
