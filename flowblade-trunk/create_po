#/usr/bin/bash

# Run create_pot before this to create teplate Flowblade.pot file before running this.
# This is assumed to run from <app_root>

# Modify variable and save script for other languages:
LANGUAGE=fi

# This command creates finnish translation pyedit.po file, modify for other languages:
msginit --output-file=Flowblade/locale/$LANGUAGE/LC_MESSAGES/Flowblade.po --input=Flowblade/locale/Flowblade/Flowblade.pot --locale=$LANGUAGE
